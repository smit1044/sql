create database xyz;
use xyz;

create table employee(
   id INT PRIMARY KEY,
   name varchar(50),
   salary INT default 25000
 );
 
 INSERT INTO employee
 (id, name, salary) 
 VALUES
(1,"ajay kumar",25000),
(2,"vijay kumar",45000),
(3,"sanjay kumar",35000),
(4,"karma kumar",75000);

select * from employee; 
